  },
  "keywords": [
    "expo",
    "expo-web",
    "json",
    "next",
    "react",
    "react-native",
    "react-dom",
    "react-native-web"
  ],
  "license": "MIT",
  "homepage": "https://github.com/next-adapter/tree/main#readme",
  "files": [
    "babel",
    "customize",
    "template",
    "build",
    "document.js",
    "document.d.ts"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^12",
    "@tsconfig/node12": "^1.0.7",
    "@types/fs-extra": "^9.0.1",
    "@types/node-fetch": "^2.5.8",
    "rimraf": "^3.0.2",
    "@types/prompts": "^2.0.6"
  },
  "peerDependencies": {
    "next": "12.1.4",
    "react": "17.0.2",
