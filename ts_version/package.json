  "description": "Adapter for using Next.js with Expo modules",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput",
    "build": "tsc",
    "prepare": "npm run clean && npm run build",
