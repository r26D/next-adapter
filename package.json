{
  "name": "@r26d/next-adapter",
  "version": "4.1.0",
  "description": "Adapter for using Next.js with Expo modules - minus type script",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run build_babel && npm run build_customize && npm run build_document && npm run build_src",
    "clean": "./node_modules/.bin/rimraf -rf babel customize document.js build",
    "lint": "eslint ..",
    "prepare": "npm run build",
    "build_babel": "npx babel js_version/babel --out-dir babel",
    "build_customize": "npx babel js_version/customize --out-dir customize",
    "build_document": "npx babel js_version/document.js --out-dir .",
    "build_src": "npx babel js_version/src --out-dir build"
  },
  "bin": {
    "next-expo": "build/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r26d/next-adapter.git",
    "directory": "packages/next-adapter"
  },
  "keywords": [
    "expo",
    "expo-web",
    "json",
    "next",
    "react",
    "react-native",
    "react-dom",
    "react-native-web"
  ],
  "license": "MIT",
  "homepage": "https://github.com/next-adapter/tree/main#readme",
  "files": [
    "README.md",
    "babel",
    "build",
    "customize",
    "document.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "rimraf": "^3.0.2",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/plugin-transform-typescript": "^7.20.2"
  },
  "peerDependencies": {
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native-web": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@expo/package-manager": "0.0.57",
    "@expo/webpack-config": "0.17.3",
    "babel-preset-expo": "^9.2.2",
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "fs-extra": "11.1.0",
    "prompts": "^2.4.2",
    "resolve-from": "^5.0.0",
    "update-check": "^1.5.4"
  }
}
